{
  "name": "generator-trailblazer",
  "version": "0.0.3",
  "description": "A pragmatic implementation of TrailsJS for rapid development",
  "keywords": [
    "yeoman-generator",
    "express",
    "expressjs",
    "trails",
    "trailsjs",
    "sails",
    "sailsjs",
    "es6"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "app"
  ],
  "scripts": {
    "clean": "make clean",
    "start": "node ./node_modules/nodemon/bin/nodemon -w ./src -w ./app/templates -x 'make yo && node ./demo/node_modules/babel-cli/bin/babel-node ./demo/server.js'",
    "build": "make build",
    "test": "node ./node_modules/nwb/lib/bin/nwb test",
    "test:coverage": "node ./node_modules/nwb/lib/bin/nwb test --coverage",
    "test:watch": "node ./node_modules/nwb/lib/bin/nwb test --server"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "yeoman-generator": "^1.0.0",
    "yeoman-option-or-prompt": "git+https://github.com/artefact-group/yeoman-option-or-prompt.git"
  },
  "devDependencies": {
    "nodemon": "^1.12.1",
    "nwb": "0.18.x"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": ""
}
