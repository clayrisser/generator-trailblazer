{
  "name": "trailblazer",
  "version": "0.0.0",
  "description": "A pragmatic implementation of TrailsJS for rapid development",
  "homepage": "https://github.com/jamrizzi/trails-imo",
  "author": {
    "name": "Jam Risser",
    "email": "jam@jamrizzi.com",
    "url": "jam.jamrizzi.com"
  },
  "main": "index.js",
  "keywords": [
    "express",
    "waterline",
    "footprints",
    "swagger",
    "trails",
    "trailsjs"
  ],
  "dependencies": {
    "change-case": "^3.0.1",
    "check-params": "^0.2.0",
    "err": "^2.1.5",
    "express": "^5.0.0-alpha.2",
    "express-custom-response": "^0.1.1",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-github2": "^0.1.10",
    "passport-jwt": "^2.2.1",
    "safe-access": "^0.1.0",
    "sails-mongo": "^0.12.2",
    "trailpack-express": "^2.0.3",
    "trailpack-footprints": "^2.0.0",
    "trailpack-repl": "v2-latest",
    "trailpack-router": "v2-latest",
    "trailpack-swagger": "git+https://github.com/jamrizzi/trailpack-swagger.git",
    "trailpack-waterline": "^2.0.0-rc2",
    "trails": "v2-latest",
    "winston": "^2.3"
  },
  "devDependencies": {
    "eslint": "^2.11",
    "eslint-config-trails": "git+https://github.com/jamrizzi/eslint-config-trails.git",
    "mocha": "^2.5",
    "supertest": "^1.2"
  },
  "scripts": {
    "test": "eslint . && mocha",
    "start": "nodemon -e js -x 'npm run lint && node server.js || exit 1'",
    "build": "docker build -t jamrizzi/oauth-trails -f ./deployment/Dockerfile .",
    "push": "docker push -t jamrizzi/oauth-trails",
    "run": "docker run --rm -p 8888:8888 jamrizzi/oauth-trails",
    "database": "docker run --name some-mongo --rm -p 27017:27017 mongo:latest",
    "lint": "eslint ."
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "eslintConfig": {
    "extends": "trails"
  },
  "repository": "jamrizzi/trails-imo",
  "license": "MIT"
}
