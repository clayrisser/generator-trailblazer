{
  "name": "generator-trailblazer",
  "version": "0.0.1",
  "description": "Describe generator-trailblazer here",
  "keywords": [
    "yeoman-generator"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "app"
  ],
  "scripts": {
    "build": "rm -rf ./demo && node ./node_modules/nwb/lib/bin/nwb build-web-module && cp -r ./lib/* ./app/",
    "yo": "export DEBUG=true && npm run build && echo && yo trailblazer",
    "clean": "rm ./app/index.js && rm -rf ./demo && node ./node_modules/nwb/lib/bin/nwb clean-module",
    "test": "node ./node_modules/nwb/lib/bin/nwb test",
    "test:coverage": "node ./node_modules/nwb/lib/bin/nwb test --coverage",
    "test:watch": "node ./node_modules/nwb/lib/bin/nwb test --server"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "yeoman-generator": "^1.0.0"
  },
  "devDependencies": {
    "nwb": "0.18.x"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": ""
}
